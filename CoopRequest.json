{
  "projectName": "Coop",
  "startUrl": "https://opskrifter.coop.dk/sitemap",
  "isFixedListOfUrls": true,
  "tasks": [
    {
      "taskName": "allRecipes",
      "nodes": [
        {
          "name": "recipe",
          "type": "object",
          "getMultipleFromPage": false,
          "isRequired": true,
          "xpath": "//article[@class='recipe']",
          "attributes": [
            {
              "name": "heading",
              "type": "string",
              "getMultipleFromPage": false,
              "isRequired": true,
              "xpath": "./header/div/div[2]/div[1]/div/div[1]/h1"
            },
            {
              "name": "ingredients",
              "type": "string",
              "getMultipleFromPage": true,
              "isRequired": true,
              "xpath": "./div[3]/div/div/section[2]/div[1]/div/div[3]/div/ul/li/span[2]/text()"
            },
            {
              "name": "image",
              "type": "object",
              "getMultipleFromPage": false,
              "isRequired": false,
              "xpath": "./header/div/div[1]/div/img",
              "attributes": [
                {
                  "name": "src",
                  "type": "string",
                  "getMultipleFromPage": false,
                  "isRequired": false,
                  "xpath": "./@src"
                },
                {
                  "name": "alt",
                  "type": "string",
                  "getMultipleFromPage": false,
                  "isRequired": false,
                  "xpath": "./@alt"
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}